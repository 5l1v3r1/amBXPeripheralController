<?xml version="1.0"?>
<!--
A test Unsynchronised scene to see what happens in certain cases
-->
<amBXScene IsExclusive="true" SceneType="Desync">
  <Frames>
    <Frame Length="500" IsRepeated="false">
      <Lights FadeTime="200">
        <West      Intensity="1" Red="1" Green="0" Blue="1" />
		<NorthWest Intensity="1" Red="1" Green="0" Blue="1" />
		<North     Intensity="1" Red="1" Green="0" Blue="1" />
		<NorthEast Intensity="1" Red="1" Green="0" Blue="1" />
		<East      Intensity="1" Red="1" Green="0" Blue="1" />
      </Lights>
    </Frame>
    <Frame Length="500" IsRepeated="true">
      <Lights FadeTime="200">
        <West      Intensity="1" Red="0" Green="1" Blue="0" />
      </Lights>
    </Frame>
	<Frame Length="500" IsRepeated="true">
      <Lights FadeTime="200">
        <NorthWest Intensity="1" Red="0" Green="1" Blue="0" />
      </Lights>
    </Frame>
	<Frame Length="500" IsRepeated="true">
      <Lights FadeTime="200">
        <North     Intensity="1" Red="0" Green="1" Blue="0" />
      </Lights>
    </Frame>
	<Frame Length="500" IsRepeated="true">
      <Lights FadeTime="200">
        <NorthEast Intensity="1" Red="0" Green="1" Blue="0" />
      </Lights>
    </Frame>
	<Frame Length="500" IsRepeated="true">
      <Lights FadeTime="200">
        <East      Intensity="1" Red="0" Green="1" Blue="0" />
      </Lights>
    </Frame>
	<!-- Now all Green, change back -->
	<Frame Length="500" IsRepeated="true">
      <Lights FadeTime="200">
        <East      Intensity="1" Red="1" Green="0" Blue="1" />
      </Lights>
    </Frame>
	<Frame Length="500" IsRepeated="true">
      <Lights FadeTime="200">
        <NorthEast Intensity="1" Red="1" Green="0" Blue="1" />
      </Lights>
    </Frame>
	<Frame Length="500" IsRepeated="true">
      <Lights FadeTime="200">
        <North     Intensity="1" Red="1" Green="0" Blue="1" />
      </Lights>
    </Frame>
	<Frame Length="500" IsRepeated="true">
      <Lights FadeTime="200">
        <NorthWest Intensity="1" Red="1" Green="0" Blue="1" />
      </Lights>
    </Frame>
	<Frame Length="500" IsRepeated="true">
      <Lights FadeTime="200">
        <West      Intensity="1" Red="1" Green="0" Blue="1" />
      </Lights>
    </Frame>
  </Frames>
</amBXScene>