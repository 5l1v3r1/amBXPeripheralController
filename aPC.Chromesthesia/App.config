<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="ChromesthesiaConfig" requirePermission="false" type="System.Configuration.NameValueSectionHandler" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
  </startup>

  <ChromesthesiaConfig>
    <add key="FftLowerDetectionFrequency" value="50" />
    <add key="FftUpperDetectionFrequency" value="10000" />
    <add key="DiagonalLightPercentageOfSide" value="80" />
    <add key="SceneFrameLength" value="10" />

    <add key="LightSectionFadeTime" value="2" />
    <add key="LightComponentMultiplicationFactor" value="3" />

    <add key="LightMaximumSamplesToUse" value="15" />
    <add key="RedMainFrequencyRange" value="50:600" />
    <add key="GreenMainFrequencyRange" value="1500:2000" />
    <add key="BlueMainFrequencyRange" value="6000:8500" />

    <add key="LightBuilderUsesNormalCDF" value="true" />
    <add key="NormalCDFNumberOfTerms" value="10" />
  </ChromesthesiaConfig>
</configuration>